generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Type {
  RESOLUCAO
  ATO
  DELIBERACAO
  NAO_ENCONTRADO
}

model Post {
  id        Int     @id
  content   String
  number    String
  type      Type
  url       String
  urlPdf    String
  embedding Float[]
}
